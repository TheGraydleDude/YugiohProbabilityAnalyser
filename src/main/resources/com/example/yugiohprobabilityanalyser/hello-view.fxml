<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.yugiohprobabilityanalyser.HelloController" prefHeight="200.0" prefWidth="300.0">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label text="Yugioh Probability Analyser" style="-fx-font-size: 20;"/>
    <Label text="Choose the .ydk file that you would like to access"/>
    <Button text="Choose File" onAction="#onFileChooseButtonClick"/>
</VBox>